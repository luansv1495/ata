{
  "name": "nata-cli",
  "version": "1.0.0",
  "description": "Architecture test automation",
  "main": "./lib/index.js",
  "bin": {
    "nata-cli": "bin/nata-cli"
  },
  "scripts": {
    "start": "npm run build && npm run global",
    "build": "npm run clean:some && tsc -p .",
    "global": "npm i -g && cd example && nata-cli .",
    "clean:some": "rm -rf ./lib",
    "lint": "eslint .",
    "prepare": "husky install",
    "test": "jest --silent"
  },
  "keywords": [
    "architectury",
    "test",
    "automation",
    "cli"
  ],
  "author": "Luan Viana",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "fs-extra": "^11.1.0",
    "glob": "^8.0.3",
    "kleur": "^4.1.5",
    "moment": "^2.29.4"
  }
}